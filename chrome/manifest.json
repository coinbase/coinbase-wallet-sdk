{
  "name": "WalletLinkDevHelper",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Injects WalletLink",
  "homepage_url": "https://wallet.coinbase.com/",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["file://*/*", "http://*/*", "https://*/*"],
      "js": ["contentScript.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
